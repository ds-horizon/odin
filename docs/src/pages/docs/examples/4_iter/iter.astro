---
import DocsLayout from '../../../../layouts/DocsLayout.astro';

const breadcrumbs = [
  { label: 'Documentation', href: '/odin/docs/introduction/overview' },
  { label: 'Examples' },
  { label: 'Iter Variable' },
];

const headings = [
  { depth: 2, slug: 'example', text: 'Example' },
];
---

<DocsLayout
  title="Iter Variable Example"
  description="Using the iter variable for sequential data"
  breadcrumbs={breadcrumbs}
  headings={headings}
>
  <h2 id="example">Example</h2>

  <pre set:html={`<code>model Product {
  fields {
    id() int
    sku() string
    category() string
  }

  gens {
    func id() {
      return iter + 1
    }

    func sku() {
      return "SKU-" + String(iter + 1000)
    }

    func category() {
      if iter % 3 == 0 {
        return "Electronics"
      } else if iter % 3 == 1 {
        return "Clothing"
      } else {
        return "Food"
      }
    }
  }
}</code>`}></pre>

  <h2>Related Documentation</h2>
  <ul>
    <li><a href="/odin/docs/concepts/advanced/iter-variable">Iter Variable</a></li>
  </ul>
</DocsLayout>

