---
import DocsLayout from '../../../../layouts/DocsLayout.astro';

const breadcrumbs = [
  { label: 'Documentation', href: '/odin/docs/introduction/overview' },
  { label: 'Examples' },
  { label: 'Misc' },
];

const headings = [
  { depth: 2, slug: 'overview', text: 'Overview' },
];
---

<DocsLayout
  title="Miscellaneous Examples"
  description="Miscellaneous datagen examples"
  breadcrumbs={breadcrumbs}
  headings={headings}
>
  <h2 id="overview">Overview</h2>

  <p>This section contains miscellaneous examples showing various datagen features.</p>

  <h3>Constants</h3>

  <pre set:html={`<code>model Config {
  misc {
    const APP_NAME = "MyApp"
    const VERSION = "1.0.0"
  }

  fields {
    appName() string
    version() string
  }

  gens {
    func appName() {
      return APP_NAME
    }

    func version() {
      return VERSION
    }
  }
}</code>`}></pre>

  <h2>Related Documentation</h2>
  <ul>
    <li><a href="/odin/docs/language/dsl-specification">DSL Specification</a></li>
  </ul>
</DocsLayout>

